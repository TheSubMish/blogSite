<%- include("partials/header") -%>
    <div>
        <h1>
            <%= title %>
        </h1>
    </div>
    <div>
        <p>
            <%= startingContent %>
        </p>
    </div>
    <div>
        <% posts.forEach(function(post){ %>
            <div>
                <h1>
                    <%= post.title %>
                </h1>
            </div>
            <div>
                <p>
                    <%= post.content.substring(0,100) + " ..." %>
                        <a href="/post/<%= post.title %>">Read more</a>
                </p>
            </div>
            <% }) %>
    </div>
    <%- include("partials/footer") -%>